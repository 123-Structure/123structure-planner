{
  "name": "123structure-planner",
  "private": true,
  "version": "0.0.0",
  "type": "commonjs",
  "main": "app/index.js",
  "description": "Planner module for 123 Structure company",
  "license": "MIT",
  "scripts": {
    "dev": "concurrently -k \"vite\" \"yarn run electron:dev\"",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "start": "npm run build && npm run electron:start",
    "electron:dev": "cross-env IS_DEV=true electron-forge start",
    "electron:package": "electron-forge package",
    "electron:build": "electron-forge make"
  },
  "dependencies": {
    "electron-squirrel-startup": "^1.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.65",
    "@electron-forge/maker-deb": "^6.0.0-beta.65",
    "@electron-forge/maker-rpm": "^6.0.0-beta.65",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.65",
    "@electron-forge/maker-zip": "^6.0.0-beta.65",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@vitejs/plugin-react": "^2.0.1",
    "concurrently": "^7.3.0",
    "cross-env": "^7.0.3",
    "electron": "20.1.1",
    "typescript": "^4.6.4",
    "vite": "^3.0.7"
  },
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "123structure_planner"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ]
    }
  }
}
