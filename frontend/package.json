{
  "name": "123structure-planner",
  "description": "Planner module for 123 Structure company",
  "author": {
    "name": "Baptiste LECHAT",
    "email": "b.lechat@123structure.fr",
    "url": "https://www.123structure.fr/"
  },
  "private": true,
  "version": "0.0.0",
  "type": "commonjs",
  "main": "app/index.js",
  "license": "MIT",
  "scripts": {
    "dev": "vite --host",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "electron:dev": "concurrently -k \"vite\" \"cross-env IS_DEV=true electron-forge start\"",
    "electron:build": "electron-forge make",
    "electron:package": "electron-forge package"
  },
  "dependencies": {
    "@emotion/react": "^11.10.4",
    "@mantine/core": "^5.2.5",
    "@mantine/dates": "^5.2.5",
    "@mantine/hooks": "^5.2.5",
    "@mantine/notifications": "^5.4.1",
    "@tabler/icons": "^1.92.0",
    "dayjs": "^1.11.6",
    "electron-squirrel-startup": "^1.0.0",
    "mantine-data-grid": "^0.0.17",
    "react": "^18.2.0",
    "react-beautiful-dnd": "^13.1.1",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dom": "^18.2.0",
    "xlsx": "https://cdn.sheetjs.com/xlsx-0.18.10/xlsx-0.18.10.tgz"
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.65",
    "@electron-forge/maker-deb": "^6.0.0-beta.65",
    "@electron-forge/maker-rpm": "^6.0.0-beta.65",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.65",
    "@electron-forge/maker-zip": "^6.0.0-beta.65",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@vitejs/plugin-react": "^3.0.0",
    "concurrently": "^7.3.0",
    "cross-env": "^7.0.3",
    "electron": "20.1.1",
    "typescript": "^4.9.3",
    "vite": "^4.0.0"
  },
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "123structure_planner"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ]
    }
  }
}
